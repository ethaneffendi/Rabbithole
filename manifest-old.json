{
  "name": "Rabbithole",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}