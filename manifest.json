{
  "name": "Rabbithole",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "mode": "module"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["proper.js"]
  }],
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ]
}