{
  "name": "Rabbithole Research",
  "description": "All the research, without the mess.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "backend.js"
  },
  "permissions": [
    "sidePanel",
    "alarms",
    "tabs",
    "scripting",
    "webNavigation",
    "notifications",
    "storage"
  ],
  "side_panel": {
    "default_path": "hello.html"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "images/icon16.png"
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
